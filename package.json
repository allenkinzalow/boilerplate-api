{
  "name": "boilerplate-api",
  "version": "0.2.0",
  "description": "A quick and easy way to setup an express api.",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./dist && ts-patch install -s && tsc",
    "lint": "eslint \"**/src/*.{js,ts}\"",
    "lint:fix": "yarn lint --fix",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all",
    "prettier": "prettier \"**/src/*.{js,ts}\"",
    "prettier:fix": "yarn prettier --write",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:reset": "npx sequelize-cli db:seed:undo:all",
    "start": "nodemon -r tsconfig-paths/register ./src/index.ts",
    "start:node": "node -r typescript-transform-paths/register ./dist/index.js",
    "pretest": "rm -f ./.sqlite/test.db && cross-env NODE_ENV=test yarn migrate && cross-env NODE_ENV=test yarn seed",
    "test": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test jest --passWithNoTests",
    "release": "changeset publish",
    "prepare": "husky install && ts-patch install -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allenkinzalow/boilerplate-api.git"
  },
  "author": "Allen Kinzalow",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/allenkinzalow/boilerplate-api/issues"
  },
  "homepage": "https://github.com/allenkinzalow/boilerplate#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "helmet": "^6.0.0",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.4",
    "tsconfig-paths": "^4.1.0",
    "typescript-transform-paths": "^3.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@changesets/cli": "^2.24.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@types/express": "^4.17.13",
    "@types/express-http-proxy": "^1.6.3",
    "@types/jest": "^28.1.7",
    "@types/node": "^17.0.25",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.7",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "jest": "^28.1.2",
    "lint-staged": "^12.4.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.4.1",
    "sqlite3": "^5.0.11",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.0.2",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
